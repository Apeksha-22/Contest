class Solution {
public:
    int minOperations(vector<int>& nums, vector<int>& target) {
        int n = nums.size();
        unordered_set<int> mp;
        mp.reserve(n * 2);

        for (int i = 0; i < n; ++i) {
            if (nums[i] != target[i])
            mp.insert(nums[i]);
        }
        vector<vector<int>> res = {nums, target};

        return static_cast<int>(mp.size());
    }
};
