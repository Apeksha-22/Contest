class Solution {
public:
    int longestSubsequence(vector<int>& nums) {
        int n=nums.size();
        int t=0;
        for(int x : nums)
        t ^= x;
        if(t !=0) return n;
        vector<int> temp= nums;
        for(int x: temp){
            if(x!=0)
            return n-1;
        }
        return 0;
    }
};
