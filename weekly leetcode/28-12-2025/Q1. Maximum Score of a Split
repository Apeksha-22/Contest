class Solution {
public:
    long long maximumScore(vector<int>& nums) {
        int n = nums.size();
        vector<long long> mn(n);
        
        mn[n-1] = nums[n-1];
        for(int i = n-2; i >= 0; i--) {
            mn[i] = min(mn[i+1], (long long)nums[i]);
        }

        long long sum = 0;
        long long res = LLONG_MIN;

        for(int i = 0; i < n-1; i++) {
            sum += nums[i];
            res = max(res, sum - mn[i+1]);
        }
        return res;
    }
};
