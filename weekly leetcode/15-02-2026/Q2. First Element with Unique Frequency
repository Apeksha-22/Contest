class Solution {
public:
    int firstUniqueFreq(vector<int>& nums) {
        unordered_map<int, int> freq;
        
        for (int x : nums)
        freq[x]++;
        
        unordered_map<int, int> cnt;
        for (auto &p : freq)
        cnt[p.second]++;
        
        for (int x : nums)
        if (cnt[freq[x]] == 1)
        return x;
        
        return -1;
    }
};
