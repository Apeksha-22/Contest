class Solution {
public:
    vector<int> toggleLightBulbs(vector<int>& bulbs) {
        vector<int> tem(101, 0);
        
        for (int x : bulbs) 
        tem[x] = 1 - tem[x];
        
        vector<int> res;
        for (int i = 1; i <= 100; i++)
        if (tem[i] == 1)
        res.push_back(i);
        
        return res;
    }
};
