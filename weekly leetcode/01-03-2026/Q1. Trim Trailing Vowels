class Solution {
public:
    string trimTrailingVowels(string s) {
        auto fun = [](char c)->bool {
            return c=='a' || c=='e' || c=='i' || c=='o' || c=='u';
        };
        int i = (int)s.size() - 1;
        while (i >= 0 && fun(s[i])) --i;
        return s.substr(0, i + 1);
    }
};
